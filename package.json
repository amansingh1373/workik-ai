{
  "name": "workik-ai",
  "displayName": "workik-AI",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "workik-ai.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "workik.refresh",
        "title": "Refresh Workik",
        "icon": "./assets/icons/refresh.svg"
      },
      {
        "command": "workik.showWebPanel",
        "title": "Workik",
        "icon": "./assets/icons/book.svg"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "workik",
          "title": "Workik AI",
          "icon": "./assets/media/workik-short-t.png"
        }
    ]},
    "views": {
      "workik": [
        {
          "id": "workik",
          "name": "",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "workik.refresh",
          "when": "view == workik",
          "group": "navigation"
        },
        {
          "command": "workik.showWebPanel",
          "when": "view == workik",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
